<h3>User's detail</h3>
	<table border="1">
		<tr>
			<td>First Name:</td>
			<td><%= @user.first_name %></td>
		</tr>
		<tr>
			<td>Last Name:</td>
			<td><%= @user.last_name %></td>
		</tr>
		<tr>
			<td>Password:</td>
			<td>Not Showing</td>
		</tr>
		<tr>
			<td>Email:</td>
			<td><%= @user.email %></td>
		</tr>
		<tr>
			<td>Landline:</td>
			<td><%= @user.landline %></td>
		</tr>
		<tr>
			<td>Mobile:</td>
			<td><%= @user.mobile %></td>
		</tr>	
		<tr>
			<td>Street:</td>
			<td><%= @address.street %></td>
		</tr>
		<tr>
			<td>Suburb:</td>
			<td><%= @address.suburb %></td>
		</tr>
		<tr>
			<td>State:</td>
			<td><%= @state.name %></td>
		</tr>
		<tr>
			<td>Postcode:</td>
			<td><%= @address.postcode %></td>
		</tr>
	</table>
